<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Edu_StoreFinderCompletion::css/store-details.css"/>
    </head>
    <body>
        <referenceBlock name="page.main.title" remove="true" />
        <referenceContainer name="content">
            <block name="StoreFinderCompletion.store"
                   class="Edu\StoreFinderCompletion\Block\StoreDetailsPage"
                   template="Edu_StoreFinderCompletion::store_details_page.phtml" />
            <block name="event.rsvp.modal"
                   class="Edu\StoreFinderCompletion\Block\RsvpPopup"
                   template="Edu_StoreFinderCompletion::events/rsvp_popup.phtml"
                   after="StoreFinderCompletion.store"/>
        </referenceContainer>
    </body>
</page>
